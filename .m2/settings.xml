<settings>
  <activeProfiles>
    <activeProfile>github</activeProfile>
  </activeProfiles>
  <servers>
      <server>
        <id>github</id>
        <username>${env.GITHUB_ACTOR}</username>
        <password>${env.GITHUB_TOKEN}</password>
      </server>
  </servers>
  <profiles>
    <profile>
      <id>github</id>
      <properties>
        <format.validate>true</format.validate>
        <codecover>true</codecover>
      </properties>
    </profile>
  </profiles>
</settings>
